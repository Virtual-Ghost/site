@font-face {
    font-family: 'Vollkorn';
    src: url('Vollkorn-Regular.woff2') format("woff2");
}
@font-face {
    font-family: 'GoudyInitialen';
    src: url('GoudyInitialen.woff2') format("woff2");
}

@font-face {
    font-family: 'enklm';
    src: url('./enklm.woff2') format("woff2");
}

h2 {font-family: 'Vollkorn','Nimbus Roman';}

h2 + p::first-letter {
 margin-top: -.0.0em;
 line-height: 0.85;
  font-size: 7em;
  //color: red;
  font-family: 'GoudyInitialen', 'Nimbus Roman';
  margin-right: 4px;
  float: left;
}


@font-face {
    font-family: 'Grimeswade';
    src: url('./Grimeswade.woff2') format("woff2");
}

@font-face {
    font-family: 'MysteriousVoyage';
    src: url('./MysteriousVoyage.woff2') format("woff2");
}

@font-face {
    font-family: 'vds3';
    src: url('./Vintage Decorative Signs 3.woff2') format("woff2");
}

@font-face {
    font-family: 'FloralCapitals';
    src: url('./FloralCapitals.woff2') format("woff2");
}

p {
  word-wrap: break-word;
  //width: 500px;
  line-height: 1.5;
  font-family: 'Vollkorn','Nimbus Roman';
  font-size: 1em;
  text-indent: 40px;
}

h2+ p::first-letter {
 margin-top: 0.11em;
 line-height: 0.85;
  font-size: 5em;
  //color: red;
  font-family: 'MysteriousVoyage', 'Nimbus Roman';
  margin-right: 4px;
  float: left;
  text-indent:0px;
}

@font-face {
    font-family: 'Pictoglyph';
    src: url('./Picto Glyphs.woff2') format("woff2");
}

h1 {font-family: 'Vollkorn','Nimbus Roman';}

h1 + p::first-letter {
 margin-top: 0.11em;
 line-height: 0.85;
  font-size: 7em;
  //color: red;
  font-family: 'Pictoglyph', 'Nimbus Roman';
  margin-right: 4px;
  float: left;
  text-indent:0px;
}

